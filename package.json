{
  "name": "vizabi-ws-reader",
  "version": "0.0.5",
  "description": "Vizabi WS reader",
  "main": "dist/bundle.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:VS-work/vizabi-ws-reader.git"
  },
  "scripts": {
    "eslint": "./node_modules/.bin/eslint --ignore-path .gitignore --ext js --fix . .config",
    "build": "npm run eslint && ./node_modules/.bin/webpack",
    "build-web": "npm run eslint && WEB_ENV=1 ./node_modules/.bin/webpack",
    "test": "npm run build && ./node_modules/.bin/ava \"test/**/*.spec.js\"",
    "prepublish": "npm run eslint && PROD_ENV=1 ./node_modules/.bin/webpack"
  },
  "author": "Valor-Software",
  "license": "GPL-3.0",
  "dependencies": {
    "async-es": "2.0.0",
    "lodash": "4.13.1"
  },
  "devDependencies": {
    "ava": "0.15.2",
    "babel": "^6.0.15",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "^6.1.2",
    "eslint": "3.0.1",
    "eslint-config-valorsoft": "0.0.16",
    "testdouble": "^1.6.0",
    "webpack": "^1.12.3"
  }
}
